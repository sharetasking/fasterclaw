{
  "name": "@fasterclaw/api-client",
  "version": "0.1.0",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "generate": "openapi-ts --input ../contracts/openapi.json --output ./src/generated --client @hey-api/client-fetch && prettier --write ./src/generated",
    "build": "pnpm generate",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hey-api/client-fetch": "^0.8.0"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.64.0",
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2"
  }
}
